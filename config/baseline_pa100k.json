{
    "n_gpu": 1,
    "model": {
        "name": "baseline",
        "backbone": "resnet50",
        "pretrained": true,
        "pooling": "gem_pooling",
        "batch_norm_bias": true
    },
    "data": {
        "data_dir": "/home/hien/Documents/datasets",
        "name": "pa_100k",
        "download": true,
        "extract": true,
        "tqdm": false,
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 4,
        "pin_memory": true,
        "drop_last": false
    },
    "optimizer": {
        "name": "sgd",
        "lr": 0.01,
        "specified_lr": {
            "lr": 0.1,
            "layers": [
                "linear",
                "bn"
            ]
        },
        "weight_decay": 0.0005,
        "momentum": 0.9,
        "sgd_dampening": 0,
        "sgd_nesterov": false
    },
    "loss": {
        "name": "CEL_Sigmoid"
    },
    "lr_scheduler": {
        "name": "ReduceLROnPlateau",
        "factor": 0.1,
        "patience": 5,
        "min_lr": 0
    },
    "clip_grad_norm_": {
        "active": true,
        "max_norm": 10.0
    },
    "trainer": {
        "epochs": 50,
        "checkpoint_dir": "saved/checkpoints",
        "log_dir": "saved/logs",
        "output_dir": "saved/outputs",
        "tqdm": true
    },
    "trainer_colab": {
        "epochs": 50,
        "checkpoint_dir": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/checkpoints",
        "log_dir": "saved/logs",
        "log_dir_saved": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/logs",
        "output_dir": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/outputs",
        "tqdm": false
    }
}