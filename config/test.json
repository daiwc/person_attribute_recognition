{
    "n_gpu": 1,
    "model": {
        "name": "baseline",
        "backbone": "resnet50_ibn_a_nl",
        "pretrained": true,
        "pooling": "avg_pooling",
        "batch_norm_bias": true
    },
    "data": {
        "data_dir": "/home/hien/Documents/datasets",
        "name": "peta",
        "download": true,
        "extract": true,
        "tqdm": false,
        "batch_size": 64,
        "shuffle": true,
        "num_workers": 4,
        "pin_memory": true,
        "drop_last": false
    },
    "optimizer": {
        "name": "adam",
        "lr": 0.00035,
        "weight_decay": 0.0005,
        "adam_beta1": 0.9,
        "adam_beta2": 0.99
    },
    "loss": {
        "name": "CEL_Sigmoid"
    },
    "lr_scheduler": {
        "name": "WarmupCosineAnnealingLR",
        "delay_iters": 30,
        "eta_min_lr": 0.00000077,
        "warmup_factor": 0.01,
        "warmup_iters": 10,
        "warmup_method": "linear"
    },
    "clip_grad_norm_": {
        "active": false,
        "max_norm": 10.0
    },
    "trainer": {
        "epochs": 120,
        "checkpoint_dir": "saved/checkpoints",
        "log_dir": "saved/logs",
        "output_dir": "saved/outputs",
        "tqdm": true
    },
    "trainer_colab": {
        "epochs": 120,
        "checkpoint_dir": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/checkpoints",
        "log_dir": "saved/logs",
        "log_dir_saved": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/logs",
        "output_dir": "/content/drive/Shared drives/REID/HIEN/Models/OSNet_Person_Attribute_Refactor/outputs",
        "tqdm": false
    }
}